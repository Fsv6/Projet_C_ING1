SET VARIABLE x = 42
SET VARIABLE y = x
SET VARIABLE z = x * 2 + 3
CURSOR 1 y z TRUE
IF CURSOR WITH ID = 1 IS IN POSITION X > 10 {
    COLOR 1 red,
    CURSOR 2 -100 -100 TRUE,
    CURSOR 3 100 100 TRUE,
    COLOR 2 green
} ELSE {
    COLOR 1 green
}
CIRCLE 1 100 2
FOR I IN RANGE (1,10) {
    MOVE 1 I,
    PASS
}